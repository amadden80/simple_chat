var pusher;
var channel;

function receive_chat(data){
  // console.log(data);
  var chats = $('#chats')
  var new_chat = $('<p>')
  new_chat.text(data)
  new_chat.appendTo(chats)

}

$(function(){

  pusher = new Pusher('<%= Pusher.key %>');
  pusher.subscribe('chatroom');
  channel = pusher.channels.channels['chatroom'];
  channel.bind( 'message', receive_chat );

})